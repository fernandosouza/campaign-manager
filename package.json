  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal-flux": "^0.1.0",
    "metal-soy": "1.0.0-rc.2"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "metal-cli": "^1.3.0"
  }
}
